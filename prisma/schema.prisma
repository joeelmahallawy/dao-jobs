// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Employers {
  id String @id @default(uuid())
  discordTag String
  ownsDao String
  profilePic String
}

model JobSeekers {
  id String @id @default(uuid())
  discordTag String
  profilePic String
}

model Daos {
  id String @id @default(uuid()) 
  nameOfDao String
  discordServerExists String
  discordLink String
  discordPopulation String
  twitterUrl String
  daoGoals String
  briefDescription String
}
